    "version": "0.2.0",
    "configurations": [
        {
            "name": "mkfs",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/mkfs.myfs",
            "args": ["container", "big.txt"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "mount",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/mount.myfs",
            "args": ["container", "logfile", "mountdir", "-s", "-f"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "unittest",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/unittest",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
